generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Product {
  id          Int    @id @default(autoincrement())
  name        String
  price       Float
  description String
  image       String
}

model Table {
id     Int    @id @default(autoincrement())
number Int    @unique
qrCode String?  // bisa berisi link atau kode unik
}


